<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Background2">#333333</Color>
    <Color x:Key="Background1">#1e1e1e</Color>
    <Color x:Key="Foreground">White</Color>

    <SolidColorBrush x:Key="Background2Brush" Color="{StaticResource Background2}" />
    <SolidColorBrush x:Key="Background1Brush" Color="{StaticResource Background1}" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource Foreground}" />
</ResourceDictionary>