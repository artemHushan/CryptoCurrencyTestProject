<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Background2">#DAD7D7</Color>
    <Color x:Key="Background1">#EDEDED</Color>
    <Color x:Key="Foreground">Black</Color>

    <SolidColorBrush x:Key="Background2Brush" Color="{StaticResource Background2}" />
    <SolidColorBrush x:Key="Background1Brush" Color="{StaticResource Background1}" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource Foreground}" />
</ResourceDictionary>